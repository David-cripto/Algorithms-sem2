#include <iostream>
#include <vector>
#include <cmath>
#include <set>
#include <string>
#include <bitset>

using std::vector;
using std::cin;
using std::cout;
using std::set;
using std::pair;
using std::make_pair;
using std::string;
using std::bitset;

const int infinity = 1;
const int self = 0;
const int bit_len = 1000;

void input(vector<vector<bitset<bit_len> >>& graph, int N);
void transitive_clousure(vector<vector<bitset<bit_len> >>& graph, int N);

int main() {
	int N;
	cin >> N;
	vector<vector<bitset<bit_len>> >graph(N);
	input(graph, N);
	transitive_clousure(graph, N);
	for (int i = 0; i < N; ++i) {
		for (int j = 0; j < N; ++j) {
			cout << graph[i][0][j];
		}
		cout << std::endl;
	}
	return 0;
}

void input(vector<vector<bitset<bit_len> >>& graph, int N) {
	string str;
	for (int i = 0; i < N; ++i) {
		cin >> str;
		string temp_str;
		int k = 0;
		while (k < str.size()) {
			if (temp_str.size() == bit_len) {
				bitset<bit_len> bits(temp_str);
				graph[i].push_back(bits);
				temp_str.clear();
			}
			temp_str.push_back(str[str.size() - 1 - k]);
			++k;
		}
		bitset<bit_len> bits(temp_str);
		graph[i].push_back(bits);
	}
}

void transitive_clousure(vector<vector<bitset<bit_len> >>& graph, int N) {
for (int k = 0; k < N; ++k) {
		for (int i = 0; i < N; ++i) {
			if (graph[i][0][k]) {
				for (int j = 0; j < graph[i].size(); ++j) {
					graph[i][j] |= graph[k][j];
				}
			}
		}
	}
}
